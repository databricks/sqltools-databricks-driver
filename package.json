{
  "name": "sqltools-databricks-driver",
  "displayName": "Databricks",
  "description": "Databricks SQL driver for SQLTools",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.42.0"
  },
  "publisher": "Databricks",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/databricks/sqltools-databricks-driver.git"
  },
  "bugs": {
    "url": "https://github.com/databricks/sqltools-databricks-driver/issues",
    "email": "bilal.aslam@databricks.com"
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "keywords": [
    "databricks",
    "databricks sql",
    "sqltools-driver"
  ],
  "categories": [
    "Lakehouse",
    "Databases"
  ],
  "extensionDependencies": [
    "mtxr.sqltools"
  ],
  "activationEvents": [
    "*",
    "onLanguage:sql",
    "onCommand:sqltools.*"
  ],
  "main": "./out/extension.js",
  "dependencies": {
    "@databricks/sql": "^0.1.8-beta.1",
    "@sqltools/base-driver": "latest",
    "@sqltools/types": "latest",
    "uuid": "^7.0.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.5.5",
    "@types/node": "^14.0.9",
    "@types/vscode": "^1.42.0",
    "typescript": "^3.7.3"
  }
}
